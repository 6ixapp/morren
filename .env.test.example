# Test Environment Configuration
# Copy this file to .env.test and update with your test database credentials

# Test Supabase Project Credentials
SUPABASE_TEST_URL=https://your-test-project.supabase.co
SUPABASE_TEST_ANON_KEY=your-test-anon-key-here

# Test Configuration
TEST_BATCH_SIZE=20
TEST_TIMEOUT=30000
TEST_MAX_USERS=20
TEST_MAX_ITEMS=25
TEST_MAX_ORDERS=15

# Performance Baselines (minimum acceptable values)
MIN_USER_RPS=5
MIN_ITEM_RPS=10
MIN_ORDER_RPS=3
MIN_BID_RPS=8
MAX_AVG_LATENCY=1000

# Safety Settings
ENABLE_CLEANUP=true
SKIP_ON_MISSING_CREDENTIALS=true